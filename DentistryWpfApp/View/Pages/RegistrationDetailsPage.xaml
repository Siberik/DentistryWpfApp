<Page x:Class="DentistryWpfApp.View.Pages.RegistrationDetailsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DentistryWpfApp.View.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="RegistrationDetailsPage">

    <Grid>
        <Viewbox>
            <StackPanel>
                <TextBlock x:Name="DateTextBlock" Margin="0,10"/>
                <TextBlock x:Name="ClientsNameTextBlock" Margin="0,10"/>
                <TextBlock x:Name="DescriptionTextBlock" Margin="0,10"/>
                <TextBlock x:Name="DentistTextBlock" Margin="0,10"/>
            </StackPanel>
            
        </Viewbox>
    </Grid>
</Page>
