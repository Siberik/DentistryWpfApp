<Page x:Class="DentistryWpfApp.View.Pages.NotesDentistPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DentistryWpfApp.View.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="NotesDentistPage" Foreground="{DynamicResource TextPrimaryColor}">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Viewbox>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
                <TextBlock Text="Дневник записей стоматолога" HorizontalAlignment="Center" VerticalAlignment="Center" Style="{StaticResource H1 TextBlock}"/>
        
            </StackPanel>
            
        </Viewbox>
        <Viewbox Grid.Row="1" >
            <StackPanel Name="VisitsStackPanel">
            
        </StackPanel>
        </Viewbox>
        
    </Grid>
</Page>
