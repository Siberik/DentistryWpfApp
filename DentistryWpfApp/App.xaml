<Application x:Class="DentistryWpfApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DentistryWpfApp"
             StartupUri="AutorizationWindow.xaml"  xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml">
    <Application.Resources>
       
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Themes/LightTheme.xaml"/>
                <ResourceDictionary Source="/Assets/Dictionaries/MainDictionary.xaml" />

               
                

                <ResourceDictionary Source="/Styles/MainStyle.xaml"/>
                <ResourceDictionary Source="/Assets/Icons/Icons.xaml"/>
                <ResourceDictionary Source="/Themes/DarkTheme.xaml"/>

               

            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
       
    </Application.Resources>
    
</Application>
